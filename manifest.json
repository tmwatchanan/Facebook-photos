{
  "manifest_version": 2,

  "name": "Facebook DOOD Album",
  "description": "DOOD photos from Facebook album",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "tabs",
    "<all_urls>"
  ]
}